{
  "openapi": "3.0.3",
  "info": {
    "title": "HelpingAI API",
    "version": "1.4"
  },
  "paths": {
    "/": {
      "get": {
        "description": "Redirect to API documentation.",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/api/search": {
      "get": {
        "summary": "Perform a text search.",
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "description": "The search query string",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "max_results",
            "in": "query",
            "description": "The maximum number of results to return",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 10,
              "minimum": 1,
              "maximum": 100
            }
          },
          {
            "name": "timelimit",
            "in": "query",
            "description": "The time limit for the search (e.g., 'd' for day, 'w' for week, 'm' for month, 'y' for year)",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "safesearch",
            "in": "query",
            "description": "The safe search level ('on', 'moderate', or 'off')",
            "required": false,
            "schema": {
              "type": "string",
              "default": "moderate"
            }
          },
          {
            "name": "region",
            "in": "query",
            "description": "The region for the search (e.g., 'us-en', 'uk-en', 'ru-ru')",
            "required": false,
            "schema": {
              "type": "string",
              "default": "wt-wt"
            }
          },
          {
            "name": "backend",
            "in": "query",
            "description": "The backend to use for search ('api', 'html', or 'lite')",
            "required": false,
            "schema": {
              "type": "string",
              "default": "api"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
