            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "layout",
            "in": "query",
            "description": "The layout of the images ('Square', 'Tall', or 'Wide')",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "license_image",
            "in": "query",
            "description": "The license of the images ('any', 'Public', 'Share', etc.)",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
